#include "../../code/io/fastin.hpp"
#include "../../code/io/fastout.hpp"
#include "../../code/poly/exp_fpssp.hpp"

CEXP u32 MOD = 998244353;

// ---<GENTC>--- begin
// ---<GENTC>--- append poly_anymod poly_nttmod
